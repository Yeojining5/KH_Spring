public class SonataSimulation {
	
	// 게으른 인스턴스화 - 시점에 대한 문제
	// 언제 초기화가 되나?
	Sonata myCar = null;
	
	// 이른 인스턴스화
	Sonata herCar = new Sonata();
	
	void methodA() {
		
	}
	
	public static void main(String[] args) {
		SonataSimulation ss = new SonataSimulation();
		ss.methodA();
	}

}
